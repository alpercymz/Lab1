a = 9
b = 4
print("Number1 and Number2 is: ")
print(a, b)
sum = a + b
diff = a - b
prod = a * b
print("The sum is: ")
print(sum)
print("The diff is: ")
print(diff)
print("The prod is: ")
print(prod)

